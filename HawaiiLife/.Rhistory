# bootMean.r
library(boot)
# This function computes the estimated sampling distribution
# sample mean using the bootstrap method.  The parameters are
#
# data - is a vector of data observations.
#
#
bootMean <- function( data, computeMean, R=5000 ){
# This internal function computes the mean of the bootstrap
# samples.
computeMean <- function( data, indecies, R ){
d <- data[indecies]
mean( d )
}
boot( data, computeMean, R )
}
